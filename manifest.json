{
  "name": "Tab query",
  "description": "Copies urls of open tabs and writes them to local storage",
  "version": "0.3",
  "permissions": ["tabs"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Tab query"
  },
  "manifest_version": 2
}